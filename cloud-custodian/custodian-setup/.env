# custodian-setup\.env

# AWS 환경 설정
ACCOUNT_ID=123456789012
AWS_REGION=ap-northeast-2

# Lambda 역할 이름 (수정 없이 기본값 사용 가능)
LAMBDA_ROLE=whs3-custodian-lambda-role
MAILER_ROLE=whs3-c7n-mailer-role

# SQS Queue URL (ACCOUNT_ID, AWS_REGION에 맞게 수정 필요)
QUEUE_URL=https://sqs.ap-northeast-2.amazonaws.com/123456789012/whs3-security-alert-queue

# Slack Webhook URL 설정
GOOD_SLACK=https://hooks.slack.com/services/T00000000/B00000000/GOOD-EXAMPLE
WARNING_SLACK=https://hooks.slack.com/services/T00000000/B00000000/WARNING-EXAMPLE
DANGER_SLACK=https://hooks.slack.com/services/T00000000/B00000000/DANGER-EXAMPLE
